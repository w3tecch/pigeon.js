{
  "name": "pigeonjs",
  "version": "0.0.4",
  "description": "This is an in-memory message bus. Very slim and fast",
  "main": "./dist/pigeon.min.js",
  "typings": "./pigeon.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/w3tecch/pigeon.js.git"
  },
  "bugs": {
    "url": "https://github.com/w3tecch/pigeon.js/issues"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "./node_modules/.bin/webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --env",
    "build:prod": "./node_modules/.bin/webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --env prod",
    "test": "./node_modules/.bin/karma start --no-auto-watch --single-run",
    "test:live": "./node_modules/.bin/karma start --auto-watch --no-single-run"
  },
  "keywords": [
    "events",
    "mediator",
    "publisher",
    "subscriber",
    "bus",
    "messaging"
  ],
  "author": "gery.hirschfeld@w3tec.ch",
  "contributors": [
    {
      "name": "David Weber",
      "url": "http://github.com/dweber19"
    }
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "assert": "^1.3.0",
    "chai": "^3.5.0",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-growl-reporter": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript-preprocessor2": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "path": "^0.12.7",
    "phantomjs": "^2.1.7",
    "phantomjs-prebuilt": "^2.1.7",
    "sinon": "^1.17.3",
    "ts-loader": "^0.8.2",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.0",
    "yargs": "^4.6.0"
  }
}
